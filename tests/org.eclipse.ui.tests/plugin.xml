               menubarPath="org.eclipse.ui.tests.internal.ListElementMenu/group1"
               id="org.eclipse.ui.tests.internal.ac9">
            <enablement>
               <pluginState
                     value="activated"
                     id="org.eclipse.update">
               </pluginState>
            </enablement>
         </action>
         <action
