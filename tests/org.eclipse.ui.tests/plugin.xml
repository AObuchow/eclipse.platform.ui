               class="org.eclipse.ui.tests.api.MockActionDelegate"
               menubarPath="org.eclipse.ui.tests.internal.ListElementMenu/group1"
               enablesFor="1"
               id="org.eclipse.ui.tests.internal.ac5">
            <enablement>
               <or>
                  <and>
                     <objectClass
                           name="org.eclipse.ui.tests.api.ListElement">
                     </objectClass>
                     <objectState
                           name="name"
                           value="red">
                     </objectState>
                  </and>
                  <and>
                     <objectClass
                           name="org.eclipse.ui.tests.api.ListElement">
                     </objectClass>
                     <objectState
                           name="name"
                           value="blue">
                     </objectState>
                  </and>
               </or>
            </enablement>
         </action>
         <action
               label="redAndTrueAction_v2"
               class="org.eclipse.ui.tests.api.MockActionDelegate"
               menubarPath="org.eclipse.ui.tests.internal.ListElementMenu/group1"
               enablesFor="1"
               id="org.eclipse.ui.tests.internal.ac6">
            <enablement>
               <and>
                  <objectClass
                        name="org.eclipse.ui.tests.api.ListElement">
                  </objectClass>
                  <objectState
                        name="name"
                        value="red">
                  </objectState>
                  <objectState
                        name="flag"
                        value="true">
                  </objectState>
               </and>
            </enablement>
         </action>
         <action
               label="pluginNotInstalledAction_v2"
               class="org.eclipse.ui.tests.api.MockActionDelegate"
               menubarPath="org.eclipse.ui.tests.internal.ListElementMenu/group1"
               id="org.eclipse.ui.tests.internal.ac7">
            <enablement>
               <pluginState
                     value="installed"
                     id="7777xxf">
               </pluginState>
            </enablement>
         </action>
         <action
               label="pluginInstalledAction_v2"
               class="org.eclipse.ui.tests.api.MockActionDelegate"
               menubarPath="org.eclipse.ui.tests.internal.ListElementMenu/group1"
               id="org.eclipse.ui.tests.internal.ac8">
            <enablement>
