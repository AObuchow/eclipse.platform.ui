            </enablement>
         </action>
         <action
               label="helloText"
               class="org.eclipse.ui.tests.api.MockActionDelegate"
               menubarPath="org.eclipse.ui.tests.internal.TextSelectionMenu/group1"
               id="org.eclipse.ui.tests.internal.ac_3">
