         <meta.section type="examples"/>
      </appInfo>
      <documentation>
         &lt;p&gt;Here is an example of some menu definitions definitions:&lt;/p&gt;
&lt;pre&gt;
	&lt;menu
		id=&quot;projection&quot;
		label=&quot;%Folding.label&quot;&gt;
		&lt;location mnemonic=&quot;%Folding.label.mnemonic&quot;&gt;
			&lt;popup path=&quot;org.eclipse.ant.internal.ui.editor.AntEditor.RulerContext/rest&quot; /&gt;
		&lt;/location&gt;
	&lt;/menu&gt;
	
	&lt;menuGroup&gt;
		&lt;location&gt; 
			&lt;popup path=&quot;org.eclipse.ant.internal.ui.editor.AntEditor.RulerContext/add&quot; /&gt;
		&lt;/location&gt;
			
		&lt;item
			id=&quot;org.eclipse.ui.texteditor.TaskRulerAction&quot;
			commandId=&quot;org.eclipse.ui.texteditor.TaskRulerAction&quot;
			helpContextId=&quot;org.eclipse.ui.AddTask_action_context&quot;&gt;
			&lt;location mnemonic=&quot;%TaskRulerAction.label.mnemonic&quot; /&gt;
		&lt;/item&gt;
		
		&lt;item
			id=&quot;org.eclipse.ui.texteditor.BookmarkRulerAction&quot;
			commandId=&quot;org.eclipse.ui.texteditor.BookmarkRulerAction&quot;
			helpContextId=&quot;org.eclipse.ui.bookmark_action_context&quot;&gt;
			&lt;location mnemonic=&quot;%BookmarkRulerAction.label.mnemonic&quot; /&gt;
		&lt;/item&gt;
	&lt;/menuGroup&gt;
&lt;/pre&gt;
      </documentation>
